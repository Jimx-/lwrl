{
  "type": "duel_dqn_agent",

  "network_spec": [
    {
      "type": "conv2d",
      "in_channels": 4,
      "out_channels": 32,
      "kernel_size": 8,
      "stride": 4,
      "activation": "relu"
    }, {
      "type": "conv2d",
      "in_channels": 32,
      "out_channels": 64,
      "kernel_size": 4,
      "stride": 2,
      "activation": "relu"
    }, {
      "type": "conv2d",
      "in_channels": 64,
      "out_channels": 64,
      "kernel_size": 3,
      "stride": 1,
      "activation": "relu"
    }, {
      "type": "flatten"
    }, {
      "type": "dense",
      "in_features": 3136,
      "out_features": 512,
      "activation": "relu"
    }
  ],

  "optimizer": {
    "type": "RMSprop",
    "args": {
      "lr": 0.00025,
      "alpha": 0.95,
      "eps": 0.01
    }
  },

  "exploration_schedule": {
    "type": "linear",
    "args": {
      "initial": 1.0,
      "final": 0.1,
      "steps": 1000000
    }
  },

  "memory": {
    "type": "sequential",
    "args": {
      "max_length": 1000000,
      "history_length": 4
    }
  },

  "state_preprocess_pipeline": [
    {
      "type": "divide",
      "scale": 255.0
    }
  ],

  "history_length": 4,
  "learning_starts": 50000,
  "train_freq": 4,
  "update_target_freq": 10000,
  "batch_size": 32,
  "discount_factor": 0.99,
  "clip_error": 1.0,
  "double_q_learning": true
}
